{
  "name": "ember-date-components",
  "version": "3.0.0",
  "description": "An Ember add-on which provides pure Ember-based date picker components.",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint ./addon ./app",
    "lint:js": "eslint .",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:one": "ember try:one"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "devDependencies": {
    "@ember/optional-features": "~1.3.0",
    "@glimmer/component": "~1.0.0",
    "@glimmer/tracking": "~1.0.0",
    "babel-eslint": "~10.1.0",
    "bootstrap-sass": "~3.4.1",
    "broccoli-asset-rev": "~3.0.0",
    "ember-auto-import": "~1.5.3",
    "ember-bootstrap": "~3.1.3",
    "ember-cli": "~3.18.0",
    "ember-cli-app-version": "~3.2.0",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-github-pages": "~0.2.0",
    "ember-cli-inject-live-reload": "~2.0.2",
    "ember-cli-moment-shim": "~3.8.0",
    "ember-cli-release-tag": "~0.1.5",
    "ember-cli-sass": "~10.0.1",
    "ember-cli-uglify": "~3.0.0",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.1",
    "ember-maybe-import-regenerator": "~0.1.6",
    "ember-moment": "~8.0.0",
    "ember-page-title": "~6.0.2",
    "ember-prism": "~0.7.0",
    "ember-qunit": "~4.6.0",
    "ember-resolver": "~8.0.0",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "~2.0.1",
    "ember-template-lint": "~2.9.1",
    "ember-try": "~1.4.0",
    "eslint": "~7.11.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-ember": "~8.5.2",
    "eslint-plugin-ember-suave": "~1.0.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.1.4",
    "husky": "~2.4.0",
    "loader.js": "~4.7.0",
    "npm-run-all": "~4.1.5",
    "precise-commits": "~1.0.2",
    "prettier": "~2.0.5",
    "qunit-dom": "~1.2.0",
    "sass": "~1.26.10"
  },
  "dependencies": {
    "ember-basic-dropdown": "^3.0.6",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^5.1.2"
  },
  "keywords": [
    "ember-addon",
    "date-picker",
    "date",
    "time",
    "time-picker",
    "timepicker",
    "date-range-picker",
    "datepicker"
  ],
  "repository": "https://github.com/mydea/ember-date-components",
  "license": "MIT",
  "author": "Francesco Novy <francesconovy@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mydea.github.io/ember-date-components/"
  },
  "volta": {
    "node": "12.16.3",
    "yarn": "1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}