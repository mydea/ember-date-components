{
  "name": "ember-date-components",
  "version": "2.0.6",
  "description": "An Ember add-on which provides pure Ember-based date picker components.",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint ./addon ./app",
    "lint:js": "eslint .",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "bootstrap-sass": "^3.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "ember-bootstrap": "^2.0.0",
    "ember-cli": "~3.9.0",
    "ember-cli-app-version": "^3.1.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-github-pages": "^0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-release-tag": "^0.1.5",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.5.0",
    "ember-page-title": "^5.0.0",
    "ember-prism": "^0.5.0",
    "ember-qunit": "^4.1.2",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.10.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^2.4.0",
    "loader.js": "^4.7.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.15.2",
    "qunit-dom": "^0.9.0",
    "sass": "^1.14.3"
  },
  "dependencies": {
    "ember-basic-dropdown": "^1.0.0",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^4.0.4"
  },
  "keywords": [
    "ember-addon",
    "date-picker",
    "date",
    "time",
    "time-picker",
    "timepicker",
    "date-range-picker",
    "datepicker"
  ],
  "repository": "https://github.com/mydea/ember-date-components",
  "license": "MIT",
  "author": "Francesco Novy <francesconovy@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mydea.github.io/ember-date-components/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}